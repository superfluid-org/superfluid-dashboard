{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
      {
        "source": "/streams/:v1Network/:tx/:log/:rest*",
        "destination": "/streams/:v1Network/:tx/:log/:rest.html"
      },
      {
        "source": "/stream/:network/:stream",
        "destination": "/stream/:network/:stream.html"
      },
      {
        "source": "/token/:network/:token",
        "destination": "/token/:network/:token.html"
      },
      {
        "source": "/vesting/:network/:id",
        "destination": "/vesting/:network/:id.html"
      }
    ],
    "redirects": [
      {
        "source": "/dashboard",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/currencies",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/transactions",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/distribution",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/activities",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/GT-Walsheim-Pro-Medium.otf",
        "destination": "/fonts/GT-Walsheim-Pro-Medium.otf",
        "statusCode": 301
      },
      {
        "source": "/GT-Walsheim-Pro-Regular.otf",
        "destination": "/fonts/GT-Walsheim-Pro-Regular.otf",
        "statusCode": 301
      },
      {
        "source": "/static/:splat*",
        "destination": "https://v1.superfluid.finance/static/:splat",
        "statusCode": 301
      },
      {
        "source": "/runner",
        "destination": "/superfluid-runner",
        "statusCode": 301
      },
      {
        "source": "/ecosystem",
        "destination": "https://www.superfluid.finance/ecosystem",
        "statusCode": 301
      }
    ],
    "headers": [
      {
        "source": "/manifest.json",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }